{
  "name": "astro-technicalseo-starter",
  "type": "module",
  "version": "1.0.0",
  "description": "Always free, production-ready Astro template with enterprise-grade performance optimization and technical SEO excellence",
  "repository": {
    "type": "git",
    "url": "https://github.com/ricardo-nth/astro-technicalSEO-starter.git"
  },
  "homepage": "https://github.com/ricardo-nth/astro-technicalSEO-starter#readme",
  "bugs": {
    "url": "https://github.com/ricardo-nth/astro-technicalSEO-starter/issues"
  },
  "author": {
    "name": "Ricardo A M Calcina",
    "url": "https://github.com/ricardo-nth"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "astro",
    "astro-template",
    "template",
    "starter",
    "seo",
    "technical-seo",
    "performance",
    "lighthouse",
    "typescript",
    "vanilla-css",
    "schema-org",
    "structured-data",
    "core-web-vitals",
    "web-performance",
    "meta-tags",
    "open-graph",
    "twitter-cards",
    "sitemap",
    "robots-txt"
  ],
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "astro": "astro",
    "analyze": "node scripts/analyze-bundle.js",
    "audit": "npm audit && astro check",
    "lighthouse": "lighthouse http://localhost:4321 --view",
    "test:accessibility": "axe-core http://localhost:4321",
    "optimize:images": "node scripts/optimize-images.js",
    "clean": "rm -rf dist .astro node_modules/.cache",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.ts,.astro",
    "precommit": "npm run check && npm run build"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/sitemap": "^3.5.1",
    "@astrojs/vercel": "^8.2.6",
    "@iconify-json/tabler": "^1.2.26",
    "@sentry/browser": "^8.39.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4",
    "astro": "^5.13.4",
    "astro-compressor": "^1.0.0",
    "astro-icon": "^1.1.5",
    "astro-seo": "^0.8.4",
    "astro-seo-schema": "^5.1.0",
    "schema-dts": "^1.1.5",
    "sharp": "^0.34.3",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.10.2",
    "lighthouse": "^12.2.1",
    "terser": "^5.43.1"
  },
  "pnpm": {
    "overrides": {
      "path-to-regexp": "^6.3.0",
      "devalue": "^5.3.2"
    }
  }
